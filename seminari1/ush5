/*
Ndërtoni një program që afishon flamurin amerikan duke përdorur karakteret * dhe =.
 */
public class Main{
    public static void main(String[] args){

   for(int i=0;i<2;i++){
       System.out.print("++++");
       for(int j=2;j<8;j++){
           System.out.print("=");
       }
       System.out.println();
   }
   for(int i=0;i<3;i++) {
       for (int j = 0; j < 10; j++) {
           System.out.print("=");
       }
       System.out.println();
   }
 }
}
